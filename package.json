{
  "name": "stacktical-developer-toolkit",
  "version": "0.0.0",
  "description": "Toolkit to deploy develop Chainlink Node, Graph Node, IPFS node, dev blockhain and DSLA contracts to multiple networks seamlessly",
  "main": "truffle-config.js",
  "scripts": {
    "hardhat-node": "hardhat node",
    "custom-ganache": "ganache-cli --accounts 10 --defaultBalanceEther 100000 -d -i 1337 -l 12000000",
    "external-adapter": "pm2 start external-adapter/index.js --name external-adapter --watch ./external-adapter --no-daemon",
    "postinstall": "hardhat typechain",
    "truffle:debug:develop": "truffle debug --network develop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stacktical/stacktical-developer-toolkit.git"
  },
  "keywords": [
    "developer",
    "dsla",
    "dsla-protocol",
    "smartcontracts",
    "chainlink",
    "graph-protocol",
    "ipfs",
    "truffle",
    "hardhat",
    "toolkit"
  ],
